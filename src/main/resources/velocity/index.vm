<!doctype html>
<html lang="en" data-framework="intercoolerjs">
    <head>
        <meta charset="utf-8">
        <title>Something Todo</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link href="https://fonts.googleapis.com/css?family=Fira+Mono:400,700|PT+Mono" rel="stylesheet">
        <link rel="stylesheet" href="/css/styles.css">
        <link rel="stylesheet" href="/css/datepicker.min.css">
    </head>
    <body>
        <header>
            <h1>something todo</h1>
        </header>
        <main class="container">
            <ol id="todo-list">
                #foreach($task in $todos)
